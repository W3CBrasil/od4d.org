<% partners.each do |partner| %>
  <article id="<%= partner.slug %>" class="partners" typeof="schema:Organization" resource="<%=partner.url%>">
    <h4 property="schema:name"><a href="<%= partner.url %>" target="_blank" class="partner-title"><%= partner.name %></a></h4>
    <%= image_tag(partner.logo, :alt => partner.name, :property => "schema:logo") %>
  </article>
<% end %>
