  background: #FC7C54 url(<%= asset_path 'social.png'%>) no-repeat 50%;
  height: 40px;
  width: 40px;
  border-radius: 50%;
  margin-right: .5em;
  margin-left: 0;
  padding: 0;
  display: inline-block;
}
.social-list-link li.twitter{
background-position: -14% 50%;
}
.social-list-link li.facebook{
background-position: 50% 50%;
}
.social-list-link li.gplus{
background-position: 113% 50%;
}
