    }
}
.maintop img{
  margin-top: .2em;
}
.maintop nav li {
  display: inline-block;
  color: #fff;
}
.maintop nav li.active {
  background: rgba(51,51,51,.45);
}
.menu li a {
  font-size: .9em;
  display: inline-block;
  vertical-align: middle;
  padding: 0 .8em;
  text-align: center;
  font-weight: 200;
  color: #fff;
  text-transform: uppercase;
  line-height: 58px;
}
.maintop nav li:hover a {
  background: rgba(51,51,51,.45);
}
.maintop nav li a.dropdown{
  padding-right: 1.5em;
  cursor: default;
}
.maintop nav li a .login{
  background: url(<%= asset_path 'icone_login.png'%>) no-repeat 50%;
  width: 41px;
  height: 58px;
  display: block;
  overflow: hidden;
  text-indent: -500px;
}
@media screen and (max-width: 780px) {
    .maintop nav li a .login{
      background: none;
      width: inherit;
      height: inherit;
      text-indent: 0px;
    }
}
.submenu li:first-child:before{
  content:"";
  width:0;
  height:0;
  border-radius: 5px;
  border-style: solid;
  border-width: 0 6px 6px 6px;
  border-color: transparent transparent #FC7C54 transparent;
  margin-left: 15px;
  margin-top: -5px;
  position: absolute;
}
.submenu li:hover:first-child:before{
  border-color: transparent transparent #e06b48 transparent;
}
.submenu{
  background: #FC7C54;
  display: none;
  z-index:999;
}
.maintop nav li:hover .submenu{
  display: block;
  position: absolute;
  min-width: 100px;
  border-radius: 0 0 5px 5px;
}
.maintop nav li:hover .submenu li{
  display: block;
}
.maintop nav li:hover .submenu a{
  background: #FC7C54;
  font-style: italic;
  text-align: left;
  padding: 0 2.2em;
  min-width: 110px;
  display: block;
  color: #000;
}
.maintop nav li:hover .submenu a:hover{
  /* background: #bf583c; */
  background: #e06b48;
  color: #fff;
}
.maintop nav li:hover .submenu li:last-child a{
  border-radius: 0 0 5px 5px;
  border-bottom: 2px solid rgba(51,51,51,.7);
}

