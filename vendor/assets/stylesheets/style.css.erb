  padding: .75em 0;
  border-bottom: none;
}
.engaje-item.integre p.be-colaborator{
  border: none;
}
.integre p:before{
  content: '';
  display: block;
  height: 93px;
  width: 93px;
  margin: 0 auto;
}
.integre p.be-partner:before{
  background: url(<%= asset_path 'be-partner.png'%>) no-repeat;
}
.integre p.be-colaborator:before{
  background: url(<%= asset_path 'be-colaborator.png'%>) no-repeat;
}
.engaje-item p:last-child{
  border-bottom: none;
}
.compartilhe p:before{
  content: '';
  height: 64px;
  width: 64px;
  display: block;
  background: url(<%= asset_path 'quote.png'%>) no-repeat;
  position: absolute;
  margin-top: -38px;
  z-index: 1;
}
.engaje-item.compartilhe p{
  border: none;
  font-size: 1.1em;
  margin: 1.3em 0 .25em;
}
.list-engaje li .engaje-item.compartilhe div{
  border: none;
  padding-bottom: .77em;
}
